{
  "surahs": [
    {
      "id": 1,
      "name": "Fatiha",
      "arabicName": "الفاتحة",
      "pronunciation": "El-Fatiha",
      "verseCount": 7,
      "meaning": "Açılış",
      "category": "mekki",
      "verses": [
        {
          "number": 1,
          "arabic": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
          "pronunciation": "Bismillahirrahmanirrahim",
          "turkish": "Rahman ve Rahim olan Allah'ın adıyla"
        },
        {
          "number": 2,
          "arabic": "الْحَمْدُ لِلَّهِ رَبِّ الْعَالَمِينَ",
          "pronunciation": "Elhamdulillahi rabbil alemin",
          "turkish": "Hamd, alemlerin Rabbi olan Allah'a mahsustur"
        },
        {
          "number": 3,
          "arabic": "الرَّحْمَٰنِ الرَّحِيمِ",
          "pronunciation": "Errahmanirrahim",
          "turkish": "Rahman ve Rahim"
        },
        {
          "number": 4,
          "arabic": "مَالِكِ يَوْمِ الدِّينِ",
          "pronunciation": "Maliki yevmiddin",
          "turkish": "Din gününün sahibi"
        },
        {
          "number": 5,
          "arabic": "إِيَّاكَ نَعْبُدُ وَإِيَّاكَ نَسْتَعِينُ",
          "pronunciation": "İyyake na'budu ve iyyake nestain",
          "turkish": "Yalnız sana ibadet ederiz ve yalnız senden yardım dileriz"
        },
        {
          "number": 6,
          "arabic": "اهْدِنَا الصِّرَاطَ الْمُسْتَقِيمَ",
          "pronunciation": "İhdinas sıratal mustakim",
          "turkish": "Bizi doğru yola ilet"
        },
        {
          "number": 7,
          "arabic": "صِرَاطَ الَّذِينَ أَنْعَمْتَ عَلَيْهِمْ غَيْرِ الْمَغْضُوبِ عَلَيْهِمْ وَلَا الضَّالِّينَ",
          "pronunciation": "Sıratallezine en'amte aleyhim ğayril mağdubi aleyhim ve lad dallin",
          "turkish": "Kendilerine nimet verdiğin kimselerin yoluna; gazaba uğrayanların ve sapıtanların yoluna değil"
        }
      ]
    },
    {
      "id": 2,
      "name": "Bakara",
      "arabicName": "البقرة",
      "pronunciation": "El-Bakara",
      "verseCount": 286,
      "meaning": "İnek",
      "category": "medeni",
      "verses": [
        {
          "number": 1,
          "arabic": "الم",
          "pronunciation": "Elif lam mim",
          "turkish": "Elif, Lam, Mim"
        },
        {
          "number": 2,
          "arabic": "ذَٰلِكَ الْكِتَابُ لَا رَيْبَ ۛ فِيهِ ۛ هُدًى لِّلْمُتَّقِينَ",
          "pronunciation": "Zalikel kitabu la raybe fih, huden lil muttakın",
          "turkish": "Bu, kendisinde şüphe olmayan kitaptır. Allah'a karşı gelmekten sakınanlar için yol göstericidir"
        },
        {
          "number": 3,
          "arabic": "الَّذِينَ يُؤْمِنُونَ بِالْغَيْبِ وَيُقِيمُونَ الصَّلَاةَ وَمِمَّا رَزَقْنَاهُمْ يُنفِقُونَ",
          "pronunciation": "Ellezine yu'minune bil ğaybi ve yukimunes salate ve mimma razaknahum yunfikun",
          "turkish": "Onlar ki gayba iman ederler, namazı dosdoğru kılarlar ve kendilerine verdiğimiz rızıktan infak ederler"
        },
        {
          "number": 4,
          "arabic": "وَالَّذِينَ يُؤْمِنُونَ بِمَا أُنزِلَ إِلَيْكَ وَمَا أُنزِلَ مِن قَبْلِكَ وَبِالْآخِرَةِ هُمْ يُوقِنُونَ",
          "pronunciation": "Vellezine yu'minune bima unzile ileyke ve ma unzile min kablike ve bil ahırati hum yukinun",
          "turkish": "Ve onlar ki sana indirilene ve senden önce indirilene iman ederler ve ahirete de kesin olarak inanırlar"
        },
        {
          "number": 5,
          "arabic": "أُولَٰئِكَ عَلَىٰ هُدًى مِّن رَّبِّهِمْ ۖ وَأُولَٰئِكَ هُمُ الْمُفْلِحُونَ",
          "pronunciation": "Ulaike ala huden min rabbihim ve ulaike humul muflihun",
          "turkish": "İşte onlar Rablerinden gelen bir hidayet üzeredirler ve kurtuluşa erenler de onlardır"
        }
      ]
    },
    {
      "id": 112,
      "name": "İhlas",
      "arabicName": "الإخلاص",
      "pronunciation": "El-İhlas",
      "verseCount": 4,
      "meaning": "Samimiyet",
      "category": "mekki",
      "verses": [
        {
          "number": 1,
          "arabic": "قُلْ هُوَ اللَّهُ أَحَدٌ",
          "pronunciation": "Kul huvallahu ehad",
          "turkish": "De ki: O Allah birdir"
        },
        {
          "number": 2,
          "arabic": "اللَّهُ الصَّمَدُ",
          "pronunciation": "Allahus samed",
          "turkish": "Allah Samed'dir"
        },
        {
          "number": 3,
          "arabic": "لَمْ يَلِدْ وَلَمْ يُولَدْ",
          "pronunciation": "Lem yelid ve lem yuled",
          "turkish": "Doğurmamış ve doğmamıştır"
        },
        {
          "number": 4,
          "arabic": "وَلَمْ يَكُن لَّهُ كُفُوًا أَحَدٌ",
          "pronunciation": "Ve lem yekun lehu kufuven ehad",
          "turkish": "Ve hiçbir şey O'na denk değildir"
        }
      ]
    },
    {
      "id": 113,
      "name": "Felak",
      "arabicName": "الفلق",
      "pronunciation": "El-Felak",
      "verseCount": 5,
      "meaning": "Tan Yeri",
      "category": "mekki",
      "verses": [
        {
          "number": 1,
          "arabic": "قُلْ أَعُوذُ بِرَبِّ الْفَلَقِ",
          "pronunciation": "Kul euzu bi rabbil felak",
          "turkish": "De ki: Tan yerinin Rabbine sığınırım"
        },
        {
          "number": 2,
          "arabic": "مِن شَرِّ مَا خَلَقَ",
          "pronunciation": "Min şerri ma halak",
          "turkish": "Yarattığı şeylerin şerrinden"
        },
        {
          "number": 3,
          "arabic": "وَمِن شَرِّ غَاسِقٍ إِذَا وَقَبَ",
          "pronunciation": "Ve min şerri ğasikın iza vekab",
          "turkish": "Karanlık bastığı zaman gecenin şerrinden"
        },
        {
          "number": 4,
          "arabic": "وَمِن شَرِّ النَّفَّاثَاتِ فِي الْعُقَدِ",
          "pronunciation": "Ve min şerrin neffasati fil ukad",
          "turkish": "Düğümlere üfleyenlerin şerrinden"
        },
        {
          "number": 5,
          "arabic": "وَمِن شَرِّ حَاسِدٍ إِذَا حَسَدَ",
          "pronunciation": "Ve min şerri hasidin iza hased",
          "turkish": "Haset ettiği zaman hasetçinin şerrinden"
        }
      ]
    },
    {
      "id": 114,
      "name": "Nas",
      "arabicName": "الناس",
      "pronunciation": "En-Nas",
      "verseCount": 6,
      "meaning": "İnsanlar",
      "category": "mekki",
      "verses": [
        {
          "number": 1,
          "arabic": "قُلْ أَعُوذُ بِرَبِّ النَّاسِ",
          "pronunciation": "Kul euzu bi rabbin nas",
          "turkish": "De ki: İnsanların Rabbine sığınırım"
        },
        {
          "number": 2,
          "arabic": "مَلِكِ النَّاسِ",
          "pronunciation": "Melikin nas",
          "turkish": "İnsanların Meliki"
        },
        {
          "number": 3,
          "arabic": "إِلَٰهِ النَّاسِ",
          "pronunciation": "İlahin nas",
          "turkish": "İnsanların İlahı"
        },
        {
          "number": 4,
          "arabic": "مِن شَرِّ الْوَسْوَاسِ الْخَنَّاسِ",
          "pronunciation": "Min şerril vesvasil hannas",
          "turkish": "Sinsi vesvesecinin şerrinden"
        },
        {
          "number": 5,
          "arabic": "الَّذِي يُوَسْوِسُ فِي صُدُورِ النَّاسِ",
          "pronunciation": "Ellezı yuvesvisu fi sudurin nas",
          "turkish": "Ki insanların göğüslerine vesvese verir"
        },
        {
          "number": 6,
          "arabic": "مِنَ الْجِنَّةِ وَالنَّاسِ",
          "pronunciation": "Minel cinneti ven nas",
          "turkish": "Cinlerden ve insanlardan"
        }
      ]
    }
  ]
}
